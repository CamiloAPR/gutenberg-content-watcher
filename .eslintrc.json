{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "linebreak-style": ["warn", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react/jsx-curly-spacing": "off",
    "react/jsx-indent": "off",
    "react/jsx-indent-props": "off"
  }
}
